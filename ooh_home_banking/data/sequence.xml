<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="seq_home_bank" model="ir.sequence">
            <field name="name">Banking Sequence</field>
            <field name="code">home.bank</field>
            <field name="prefix">BNK</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_home_bank_line" model="ir.sequence">
            <field name="name">Banking Line Sequence</field>
            <field name="code">home.bank.line</field>
            <field name="prefix">TRC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
     </data>
</odoo>
